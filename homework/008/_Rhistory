points(H9P2.predict$fit~xs, type="l", col="red")
lines(H9P2.predict$fit+H9P2.predict$se.fit ~ xs, col="gray", type="l", lty=2)
lines(H9P2.predict$fit-H9P2.predict$se.fit ~ xs, col="gray", type="l", lty=2)
